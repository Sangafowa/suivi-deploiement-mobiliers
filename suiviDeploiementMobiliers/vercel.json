{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/suivi-deploiement-mobiliers",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "outputDirectory": "dist/suivi-deploiement-mobiliers"
}
